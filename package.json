{
  "name": "le-grid",
  "version": "0.0.13-pre",
  "description": "Reactive grid in Dojo 2",
  "scripts": {
    "build-app": "dojo build",
    "precommit": "lint-staged",
    "prettier": "prettier --write 'src/**/*.ts*' 'src/**/*.css*' 'tests/**/*.ts*' 'examples/**/*.ts*'",
    "test": "grunt test",
    "examples": "npm run examples:package && npm run examples:install && npm run examples",
    "examples:package": "shx rm -rf dist && grunt dist && grunt release-publish-flat --dry-run && shx mv dist/le-grid-* dist/le-grid.tgz",
    "examples:install": "cd examples && shx rm -rf node_modules/le-grid && npm install && cd ..",
    "examples:build": "cd examples && npm run build && cd ..",
    "examples:watch": "cd examples && npm run watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/agubler/le-grid"
  },
  "bugs": {
    "url": "https://github.com/agubler/le-grid/issues"
  },
  "license": "MIT",
  "author": "Anthony Gubler",
  "dependencies": {
    "@dojo/core": "^2.0.0",
    "@dojo/has": "^2.0.0",
    "@dojo/shim": "^2.0.0",
    "@dojo/i18n": "^2.0.0",
    "@dojo/widget-core": "^2.0.0",
    "@dojo/stores": "^2.0.0",
    "tslib": "~1.8.1"
  },
  "devDependencies": {
    "@dojo/test-extras": "^2.0.0",
    "@types/execa": "^0.9.0",
    "@types/glob": "~5.0.0",
    "@types/grunt": "~0.4.0",
    "@types/node": "^6.0.46",
    "@types/sinon": "^1.16.35",
    "chai": "~3.5.0",
    "execa": "^0.10.0",
    "grunt": "^1.0.1",
    "grunt-dojo2": "^0.1.3",
    "grunt-tslint": "5.0.1",
    "husky": "0.14.3",
    "intern": "~4.1.5",
    "lint-staged": "6.0.0",
    "load-grunt-tasks": "^4.0.0",
    "prettier": "1.9.2",
    "shx": "^0.2.2",
    "sinon": "^1.17.7",
    "ts-node": "^6.0.3",
    "tslint": "5.2.0",
    "typescript": "~2.6.1"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "tabWidth": 4,
    "useTabs": true,
    "printWidth": 120,
    "arrowParens": "always"
  }
}
